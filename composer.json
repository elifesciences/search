{
    "name": "elife/search",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "eLife\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\eLife\\": "./tests/src"
        }
    },
    "require": {
        "php": "^7.4",
        "aws/aws-sdk-php": "^3.19",
        "clue/block-react": "^1.1",
        "doctrine/cache": "^1.6",
        "elasticsearch/elasticsearch": "^7.11",
        "elife/api": "^2.30",
        "elife/api-client": "dev-master",
        "elife/api-problem": "^1.1",
        "elife/api-sdk": "^1.0@dev",
        "elife/api-validator": "dev-master",
        "elife/bus-sdk": "dev-master",
        "elife/content-negotiator": "^1.0.3",
        "elife/logging-sdk": "^1.0@dev",
        "elife/ping": "^1.0.1",
        "guzzlehttp/guzzle": "^6.3",
        "jms/serializer": "^1.5@dev",
        "justinrainbow/json-schema": "^5.1",
        "mindplay/composer-locator": "^2.1",
        "monolog/monolog": "^1.22",
        "silex/silex": "^2.3",
        "symfony/console": "4.4.49",
        "symfony/finder": "5.4.43",
        "symfony/monolog-bridge": "^4.4",
        "symfony/psr-http-message-bridge": "^1.0",
        "symfony/yaml": "^4.4",
        "willdurand/negotiation": "^3.1",
        "nyholm/psr7": "^1.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.5",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/browser-kit": "^4.4",
        "symfony/var-dumper": "^4.4"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "mindplay/composer-locator": true
        }
    }
}
