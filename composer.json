    "autoload-dev": {
        "psr-4": {
            "tests\\eLife\\": "./tests/src"
        }
    },
    "require": {
        "php": "8.3.*",
        "aws/aws-sdk-php": "3.336.10",
        "beberlei/assert": "^3.3",
        "doctrine/annotations": "^1.4",
        "elasticsearch/elasticsearch": "^7.11",
        "elife/api": "^2.30",
        "elife/api-client": "^1.1",
        "elife/api-problem": "^1.1",
        "elife/api-sdk": "^1.0@dev",
        "elife/api-validator": "^1.1",
        "elife/bus-sdk": "^1.3",
        "elife/content-negotiator": "^1.1",
        "elife/logging-sdk": "^1.2",
        "elife/ping": "^1.2",
        "guzzlehttp/guzzle": "^7.3",
        "jms/serializer": "^3.0",
        "justinrainbow/json-schema": "^6.0",
        "mindplay/composer-locator": "^2.1",
        "nyholm/psr7": "^1.1",
        "pimple/pimple": "^3.5",
        "psr/container": "^1.1",
        "psr/log": "^2.0",
        "silexandfriends/silex": "^3.0",
        "symfony/console": "^7.0",
        "symfony/http-foundation": "^7.0",
        "symfony/http-kernel": "^7.0",
        "symfony/monolog-bridge": "^7.0",
        "symfony/psr-http-message-bridge": "^7.0",
        "symfony/serializer": "^7.0",
        "symfony/yaml": "^7.0",
        "willdurand/negotiation": "^3.1"
    },
    "require-dev": {
        "guzzlehttp/promises": "^1.5",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^11.0",
        "shipmonk/composer-dependency-analyser": "^1.8",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/browser-kit": "^7.0",
        "symfony/finder": "7.3.2",
        "symfony/var-dumper": "^7.0"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "mindplay/composer-locator": true
        }
    }
}
