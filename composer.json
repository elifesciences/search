{
    "name": "elife/search",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "eLife\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\eLife\\": "./tests/src"
        }
    },
    "require": {
        "php": "^7.4",
        "aws/aws-sdk-php": "^3.19",
        "clue/block-react": "^1.1",
        "doctrine/cache": "^1.6",
        "elasticsearch/elasticsearch": "^7.11",
        "elife/api": "^2.21",
        "elife/api-client": "dev-master",
        "elife/api-problem": "^1.1",
        "elife/api-sdk": "dev-master",
        "elife/api-validator": "dev-master",
        "elife/bus-sdk": "dev-master",
        "elife/content-negotiator": "^1.0",
        "elife/logging-sdk": "^1.0@dev",
        "elife/ping": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "jms/serializer": "^1.5@dev",
        "justinrainbow/json-schema": "^5.1",
        "mindplay/composer-locator": "^2.1",
        "monolog/monolog": "^1.22",
        "propel/propel": "^2.0@alpha",
        "silex/silex": "^2.0",
        "symfony/event-dispatcher": "^3.2",
        "symfony/monolog-bridge": "^3.2",
        "symfony/psr-http-message-bridge": "^0.2.0",
        "willdurand/negotiation": "^2.3",
        "zendframework/zend-diactoros": "^1.3"
    },
    "require-dev": {
        "csa/guzzle-cache-middleware": "^1.0",
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "^5.5",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/browser-kit": "^3.1",
        "symfony/var-dumper": "^3.1"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "mindplay/composer-locator": true
        }
    }
}
