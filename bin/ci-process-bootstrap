# propagate SIGINT to waiting processes

# lsh@2022-01: superceded
# trap 'kill -INT -$waitingPid' INT

interrupt_fn() {
    echo "interrupt caught, exiting."
    kill "$waitingPid" || echo "'waitingPid' not set"
    kill "$workerPid" || echo "'workerPid' not set"
    kill "$queueWatchPid" || echo "'queueWatchPid' not set"
}

trap interrupt_fn EXIT
